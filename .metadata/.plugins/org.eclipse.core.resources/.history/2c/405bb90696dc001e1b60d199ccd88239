package com;

public class Tutorial5 {

	public static void happyNumber(int x) {
		
	double num = x;
	double number = 0;
	while(num >1) {
		
		double rem = num % 10;
		
 number += Math.pow(rem,2);
 
 
 num/=10;
 
 
	}
		
		System.out.println(number);
		
		
	}
	
	
	
	
	
	public static void main(String[] args) {
		
		happyNumber(13);

	}

}
