package com;

import java.util.Iterator;

public class Tutorial2 {

	public Tutorial2() {
		// TODO Auto-generated constructor stub
	}

	
	public static void main(String[] args) {
		
		//this is programming about finding max and min
		
		int [] arr = {5,3,4,7,9};
		
		for (int i = 0; i < arr.length-1; i++) {
			
			if(arr[i]> arr[i+1]) {
				
				arr[i]=arr[i+1];
			}
		}
		
	}
	
}
