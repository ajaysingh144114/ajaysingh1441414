package com;

import java.util.*;

public class Game {
	 public static  boolean isPalindrome(int x) {
	        int num = x;
	        int rev = 0;
	        while(num > 0){
	         int rem = num % 10;
	         rev += rem * 10;
	        num /=10;
	        }
	    if (rev == x)
	    	return true;
	    return false;
	 

	    }
	public static void main(String[] args) {
		
		    boolean x =   isPalindrome(121);
		    
		    System.out.println(x);
		      
		
		
	}

}
